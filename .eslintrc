{
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": 0,
    "camelcase": 0,
    "new-cap": 0,
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "no-unused-vars": [
      2,
      {
        "vars": "all",
        "args": "after-used",
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ],
    "semi": [
      2,
      "always"
    ],
    "no-console": 0,
    "id-length": [
      2,
      {
        "min": 2,
        "exceptions": [
          "x",
          "v",
          "T",
          "_"
        ]
      }
    ]
  },
  "env": {
    "es6": true,
    "node": true,
    "mocha": true
  },
  "ecmaFeatures": {
    "classes": true,
    "modules": true
  },
  "plugins": [
  ],
  "extends": "airbnb/base"
}